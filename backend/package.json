{
  "name": "backend",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "npm run prod",
    "build": "npm-run-all clean transpile",
    "src": "node ./dist/index",
    "dev": "NODE_ENV=development npm-run-all build src",
    "prod": "NODE_ENV=production npm-run-all build src",
    "transpile": "babel ./src --out-dir dist",
    "clean": "rimraf dist",
    "watch:dev": "nodemon",
    "test": "jest"
  },
  "dependencies": {
    "@babel/cli": "^7.13.10",
    "@babel/core": "^7.13.10",
    "@babel/preset-env": "^7.13.10",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "express": "^4.16.4",
    "express-graphql": "^0.12.0",
    "graphql": "^15.5.0",
    "graphql-relay": "^0.6.0",
    "graphql-sequelize": "^9.4.3",
    "graphql-tools": "^7.0.4",
    "http-errors": "~1.6.3",
    "jade": "~1.11.0",
    "morgan": "~1.9.1",
    "mysql2": "^2.2.5",
    "nodemon": "^2.0.7",
    "npm-run-all": "^4.1.5",
    "pg": "^8.5.1",
    "pg-hstore": "^2.3.3",
    "regenerator-runtime": "^0.13.7",
    "rimraf": "^3.0.2",
    "sequelize": "^6.5.0",
    "uninstall": "0.0.0"
  },
  "devDependencies": {
    "@babel/plugin-transform-runtime": "^7.13.10",
    "babel-core": "^6.26.3",
    "babel-loader": "^8.2.2",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "eslint": "^7.22.0",
    "eslint-plugin-react": "^7.22.0",
    "jest": "^26.6.3"
  },
  "nodemonConfig": {
    "exec": "npm run dev",
    "watch": [
      "src/*",
      "public/*"
    ],
    "ignore": [
      "**/__tests__/**",
      "*.test.js",
      "*.spec.js"
    ]
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ],
    "plugins": [
      "@babel/plugin-transform-runtime"
    ]
  },
  "jest": {
    "testEnvironment": "node"
  }
}
